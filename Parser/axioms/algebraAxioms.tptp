fof(reflexive, axiom, ((
	![X:$i]:
	equals(X,X)))).

fof(symmetric, axiom, ((
	![X:$i,Y:$i]:(
	equals(X,Y)
	=> equals(Y,X))))).

fof(transitive, axiom, ((
	![A:$i,B:$i,C:$i]:(
	equals(A,B)
	& equals(B,C)
	=> equals(A,C))))).

fof(antiTransitivity, axiom, ((
	![A:$i,B:$i,C:$i]:(
	equals(A,B)
	& ~equals(B,C)
	=> ~equals(A,C))))).

fof(substitution_multi, axiom, ((	
	![A:$i,B:$i,C:$i,D:$i,E:$i,F:$i]:(
	equals(A,B)
	& equals(C,D)
	=> equals(product(A,C),product(B,D)))))).

fof(substitution_add, axiom, ((
	! [A:$i,B:$i,C:$i,D:$i,E:$i,F:$i]:
	(equals(A,B)
	& equals(C,D)
	=> equals(sum(A,C),sum(B,D)))))).

fof(multIdentity, axiom, ((
	?[_1:$i]:(
	![X:$i]:(
	equals(X,product(_1,X))))))).	

fof(addIdentity, axiom, ((
	?[_0:$i]:(
	![X:$i]:(
	equals(X,sum(_0,X))))))).	

fof(addSymm, axiom, ((
	![X:$i,Y:$i]:(
	equals(sum(X, Y),sum(Y, X)))))).

fof(multSymm, axiom, ((
	![X:$i,Y:$i]:(
	equals(product(X, Y),product(Y, X)))))).

fof(parensMulti, axiom, ((
	![Z:$i,X:$i,Y:$i]:(
	equals(product(product(X,Y),Z),product(X,product(Y,Z))))))).

fof(addMulti, axiom, ((
	![Z:$i,X:$i,Y:$i]:(
	equals(sum(sum(X,Y),Z),sum(X,sum(Y,Z))))))).

