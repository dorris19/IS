fof(extensionality, axiom,((
	(member(A,X))
	& (member(A,Y))
	=> X = Y))).

fof(transitive, axiom,((

	(subset(X, Y))
	& (subset(Y, Z))
	=> subset(X,Z)))).

fof(subsetDef, axiom,((

	(subset(X, Y))
	& (member(A, X))
	=> member(A, Y)))).

fof(extensionality, axiom,((
	(X = Y)
	& member(B, Y)
	=>
	member(B, X)))).

fof(regularity, axiom,((
	member(X, Y)
	=> ~(X = Y)))).
